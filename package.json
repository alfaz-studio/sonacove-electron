{
  "name": "sonacove-meets",
  "productName": "Sonacove Meets",
  "author": "Alfaz Studio",
  "version": "2025.15.52",
  "description": "Sonacove Meeting App",
  "main": "./build/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/alfaz-studio/sonacove-electron.git"
  },
  "scripts": {
    "start": "cross-env APP_ENV=staging webpack --config ./webpack.main.js --mode development && concurrently \"npm:watch\" \"electron .\"",
    "start:prod": "cross-env APP_ENV=production webpack --config ./webpack.main.js --mode development && concurrently \"npm:watch:prod\" \"electron .\"",
    "clean": "rm -rf node_modules build dist",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "build": "webpack --config ./webpack.main.js --mode production && webpack --config ./webpack.renderer.js --mode production",
    "dist": "npm run dist:prod",
    "dist:prod": "cross-env APP_ENV=production npm run build && electron-builder --config electron-builder-prod.json",
    "dist:staging": "cross-env APP_ENV=staging npm run build && electron-builder --config electron-builder-staging.json",
    "postinstall": "patch-package",
    "watch": "cross-env APP_ENV=staging webpack --config ./webpack.renderer.js --mode development --watch --watch-options-poll=1000",
    "watch:prod": "cross-env APP_ENV=production webpack --config ./webpack.renderer.js --mode development --watch --watch-options-poll=1000"
  },
  "dependencies": {
    "@jitsi/electron-sdk": "9.0.7",
    "electron-debug": "^3.2.0",
    "electron-log": "^5.0.0",
    "electron-reload": "^1.5.0",
    "electron-updater": "6.6.2",
    "electron-window-state": "^5.0.3",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@atlaskit/button": "^10.1.3",
    "@atlaskit/css-reset": "^3.0.8",
    "@atlaskit/droplist": "^7.0.19",
    "@atlaskit/field-text": "^7.1.0",
    "@atlaskit/icon": "^15.0.3",
    "@atlaskit/navigation": "^33.3.10",
    "@atlaskit/onboarding": "^6.2.0",
    "@atlaskit/page": "^8.0.12",
    "@atlaskit/spinner": "^9.0.13",
    "@atlaskit/theme": "^7.0.5",
    "@atlaskit/toggle": "^5.0.15",
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-proposal-export-namespace-from": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@hapi/bourne": "^3.0.0",
    "@jitsi/eslint-config": "^6.0.4",
    "@jitsi/js-utils": "2.0.5",
    "@svgr/webpack": "^6.2.1",
    "babel-eslint": "10.0.3",
    "babel-loader": "^8.2.3",
    "concurrently": "5.1.0",
    "cross-env": "^10.1.0",
    "css-loader": "^6.7.1",
    "electron": "30.1.0",
    "electron-builder": "26.0.12",
    "electron-context-menu": "^2.5.0",
    "electron-is-dev": "^1.2.0",
    "electron-react-devtools": "0.5.3",
    "eslint": "^8.57.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-react": "^7.37.0",
    "file-loader": "^6.2.0",
    "history": "^4.10.1",
    "html-webpack-plugin": "^5.5.0",
    "i18next": "^19.9.2",
    "moment": "^2.29.2",
    "mousetrap": "^1.6.5",
    "patch-package": "8.0.1",
    "rcedit": "^5.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.8.12",
    "react-redux": "^5.1.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "source-map-support": "^0.5.19",
    "style-loader": "^3.3.1",
    "styled-components": "^3.4.10",
    "webpack": "^5.94.0",
    "webpack-cli": "^4.9.2"
  }
}
